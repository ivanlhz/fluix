{
  "name": "@fluix-ui/angular",
  "version": "0.0.5",
  "description": "Angular adapter for Fluix UI components.",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@angular/common": ">=17",
    "@angular/core": ">=17",
    "@fluix-ui/core": "workspace:*",
    "@fluix-ui/css": "workspace:*",
    "rxjs": ">=7"
  },
  "devDependencies": {
    "@angular/common": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@fluix-ui/core": "workspace:*",
    "@fluix-ui/css": "workspace:*",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ivanlhz/fluix.git"
  },
  "publishConfig": {
    "access": "public"
  }
}